prefix=${pcfiledir}/../..
libdir=${prefix}/lib
includedir=${prefix}/include/

Name: Open3D
Description: Open3D: A Modern Library for 3D Data Processing.
URL: https://www.open3d.org
Version: 0.19.0
Cflags: -std=c++17 -isystem${includedir} -isystem${includedir}/open3d/3rdparty -D_GLIBCXX_USE_CXX11_ABI=0 -D_GLIBCXX_USE_CXX11_ABI=0 -DFMT_HEADER_ONLY=0 -DFMT_USE_WINDOWS_H=0 -DFMT_STRING_ALIAS=1
Libs: -L${libdir} -Wl,-rpath,${libdir} -lOpen3D -ltbb
